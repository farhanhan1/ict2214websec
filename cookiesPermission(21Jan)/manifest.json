{
  "manifest_version": 2,
  "name": "Comprehensive Cookie Manager",
  "version": "1.0",
  "description": "Manage and secure your cookies",
  "permissions": [
      "cookies",
      "storage",
      "<all_urls>",
      "storage",
      "<your-web-accessible-domain>",
      "tabs" 
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  },
  "icons": {
      "48": "icon.png"
  }
}
